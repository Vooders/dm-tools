{
  "name": "dm-tools",
  "productName": "dm-tools",
  "version": "0.12.2",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "postinstall": "electron-builder install-app-deps",
    "build": "electron-forge make",
    "release": "electron-builder --mac --windows --linux --publish always",
    "pretest": "rm -rf js && tsc",
    "test": "mocha --recursive js/test"
  },
  "keywords": [],
  "author": {
    "name": "Vooders",
    "email": "kev.wilson84@gmail.com"
  },
  "build": {
    "appId": "com.vooders.dm-tools",
    "productName": "DM Tools",
    "mac": {
      "category": "public.app-category.games"
    },
    "dmg": {
      "icon": false
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Game"
    },
    "files": [
      "**/*"
    ],
    "asar": null
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "dm_tools"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.js"
                  }
                }
              ]
            },
            "loggerPort": 9001
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.66",
    "@electron-forge/maker-deb": "^6.0.0-beta.66",
    "@electron-forge/maker-rpm": "^6.0.0-beta.66",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.66",
    "@electron-forge/maker-zip": "^6.0.0-beta.66",
    "@electron-forge/plugin-webpack": "6.0.0-beta.66",
    "@electron-forge/publisher-github": "^6.0.0-beta.67",
    "@types/chai": "^4.3.6",
    "@types/chai-as-promised": "^7.1.6",
    "@types/mocha": "^10.0.1",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "chai": "^4.3.8",
    "chai-as-promised": "^7.1.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.0.0",
    "electron": "21.0.1",
    "electron-builder": "^23.6.0",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "mocha": "^10.2.0",
    "node-loader": "^2.0.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@babel/core": "^7.19.3",
    "@electron/remote": "^2.0.8",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.103",
    "@mui/material": "^5.10.7",
    "@types/path-browserify": "^1.0.0",
    "electron-fetch": "^1.9.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^5.3.0",
    "js-base64": "^3.7.4",
    "node-fetch": "^3.2.10",
    "path-browserify": "^1.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "recharts": "^2.1.14",
    "webpack": "^5.74.0"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
